#!/usr/bin/env manage
#
# vim: set ft=sh:
#
# <doc>
#
# copy
#
# </doc>
#
# <import>
#
# collection-shell
# collection-message
# collection-string
#
# </import>

main ()
{
    verbose true
    strict true

    if _ truth "$DOCKER"; then
        _ info "Copying release ..."

        [ -d "${MANAGEREPOSITORY}/release/$1" ] && cp -rf "${MANAGEREPOSITORY}/release/$1"/* /release
    fi
}
